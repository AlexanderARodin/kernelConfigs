CONFIG_DEFAULT_HOSTNAME="raaNux"
CONFIG_64BIT=y
CONFIG_MODULES=y
CONFIG_MODULE_UNLOAD=y
CONFIG_MODULE_FORCE_UNLOAD=y

#
# COMMON: critical
#

#
# [ kernel should speak ]
#
CONFIG_PRINTK=y
CONFIG_PRINTK_TIME=y
CONFIG_TTY=y
# CONFIG_UNIX98_PTYS is not set
# CONFIG_LEGACY_PTYS is not set
CONFIG_BLK_DEV_INITRD=y
CONFIG_BINFMT_ELF=y
CONFIG_BINFMT_SCRIPT=y
# end of COMMON: critical

#
# COMMON: usable
#
CONFIG_PROC_FS=y
CONFIG_SYSFS=y
CONFIG_DEVTMPFS=y
CONFIG_DEVTMPFS_MOUNT=y

#
# [ others ]
#
CONFIG_BUG=y
CONFIG_SERIO_SERPORT=m
CONFIG_HID=m
# end of COMMON: usable

CONFIG_FOR_QEMU_GROUP=y

#
# minimal
#

#
# CPU
#

#
# < Intel 06/9e (family: 0x6, model: 0x9e, stepping: 0xd) >
#
CONFIG_SMP=y
# CONFIG_X86_MPPARSE is not set
CONFIG_NR_CPUS=4
CONFIG_HYPERVISOR_GUEST=y
CONFIG_PARAVIRT=y
CONFIG_PARAVIRT_SPINLOCKS=y
CONFIG_PARAVIRT_TIME_ACCOUNTING=y

#
# [ others ]
#
CONFIG_LEGACY_VSYSCALL_NONE=y
# end of CPU

#
# BOARD & BUS
#
CONFIG_RTC_CLASS=y
CONFIG_DMI=y
CONFIG_ACPI=y
CONFIG_ACPI_BUTTON=m
# CONFIG_ACPI_AC is not set
# CONFIG_ACPI_BATTERY is not set
# CONFIG_ACPI_FAN is not set
# CONFIG_ACPI_THERMAL is not set

#
# BUS
#
CONFIG_PCI=y
CONFIG_VIRTIO_MENU=y
CONFIG_VIRTIO_PCI=y
# end of BOARD & BUS

#
# network
#
CONFIG_IPV6=m
CONFIG_NET=y
CONFIG_INET=y
CONFIG_NETDEVICES=y
CONFIG_NET_FAILOVER=y
CONFIG_VIRTIO_NET=m
# CONFIG_WIRELESS is not set
# CONFIG_WLAN is not set
# CONFIG_USB_NET_DRIVERS is not set
# CONFIG_ETHERNET is not set

#
# [ 9p support ]
#
CONFIG_NET_9P=y
CONFIG_NET_9P_VIRTIO=y
CONFIG_9P_FS=m
CONFIG_9P_FS_POSIX_ACL=y

#
# [ others ]
#
CONFIG_POSIX_TIMERS=y
# end of network

#
# Input devices
#
CONFIG_INPUT_MOUSEDEV=m
CONFIG_MOUSE_PS2=m
CONFIG_USERIO=m
CONFIG_INPUT_TABLET=y
# end of Input devices
# end of minimal

#
# recommended
#
CONFIG_LPC_ICH=y
CONFIG_I2C_I801=y
CONFIG_BLOCK=y

#
# disks
#
CONFIG_ATA=y
CONFIG_SATA_AHCI=y
CONFIG_SCSI=y
CONFIG_BLK_DEV_SD=y
CONFIG_BLK_DEV_SR=y
CONFIG_CHR_DEV_SG=y
CONFIG_SCSI_VIRTIO=m
CONFIG_VIRTIO_BLK=m
# end of disks

#
# block FS
#
CONFIG_EXT2_FS=y
CONFIG_EXT4_FS=m
CONFIG_EXT4_FS_POSIX_ACL=y

#
# [ CD-ROM/DVD ]
#
CONFIG_ISO9660_FS=m
CONFIG_JOLIET=y
CONFIG_ZISOFS=y
CONFIG_UDF_FS=m

#
# [ Windows FATs ]
#
CONFIG_VFAT_FS=m
CONFIG_FAT_DEFAULT_IOCHARSET="ascii"
CONFIG_FAT_DEFAULT_UTF8=y
CONFIG_EXFAT_FS=m
# end of block FS

CONFIG_NETWORK_FILESYSTEMS=y
CONFIG_CIFS=m
CONFIG_CIFS_XATTR=y
CONFIG_CIFS_POSIX=y

#
# FS language support
#
CONFIG_NLS=y
CONFIG_NLS_DEFAULT="utf8"
CONFIG_NLS_ASCII=m
CONFIG_NLS_UTF8=m
CONFIG_UNICODE=y
# end of FS language support

#
# hardware RANDOM
#
CONFIG_HW_RANDOM=y
CONFIG_HW_RANDOM_VIRTIO=y
# CONFIG_HW_RANDOM_INTEL is not set
# CONFIG_HW_RANDOM_AMD is not set
# CONFIG_HW_RANDOM_VIA is not set
# end of hardware RANDOM

#
# Graphics
#
# CONFIG_VGA_ARB is not set
CONFIG_DRM=y
CONFIG_DRM_BOCHS=y
CONFIG_DUMMY_CONSOLE_COLUMNS=128
CONFIG_DUMMY_CONSOLE_ROWS=48
# end of Graphics
# end of recommended

#
# extended
#
# CONFIG_MULTIUSER is not set

#
# [ virtIO ]
#
CONFIG_VIRTIO_CONSOLE=m
CONFIG_VIRTIO_INPUT=m
CONFIG_CRYPTO_DEV_VIRTIO=m
CONFIG_VSOCKETS=m

#
# [ socket monitoring ]
#
CONFIG_INET_DIAG=m
CONFIG_INET_UDP_DIAG=m
CONFIG_INET_RAW_DIAG=m

#
# [ CPU features ]
#
CONFIG_X86_MSR=m
CONFIG_HALTPOLL_CPUIDLE=m
